landing:
  loginUrl: http://{{ .Values | get "host" "localhost" }}/cityos/

ingress:
  hosts:
    - host: {{ .Values | get "host" "localhost" }}
      paths:
        - path: /landing/
          pathType: Prefix
  tls:
    enabled: {{ .Values | get "tls.enabled" false }}
    secretName: host-tls
    issuer: {{ .Values | get "tls.issuer" "letsencrypt-prod" }}
    hosts:
      - host: {{ .Values | get "host" "localhost" }}